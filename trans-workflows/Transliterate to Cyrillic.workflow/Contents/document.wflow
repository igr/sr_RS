	set input to replace_chars(input, "lj", "љ")	set input to replace_chars(input, "m", "м")	set input to replace_chars(input, "n", "н")
	set input to replace_chars(input, "nj", "њ")	set input to replace_chars(input, "o", "о")	set input to replace_chars(input, "p", "п")	set input to replace_chars(input, "r", "р")	set input to replace_chars(input, "s", "с")	set input to replace_chars(input, "t", "т")
	set input to replace_chars(input, "ć", "ћ")		set input to replace_chars(input, "u", "у")	set input to replace_chars(input, "f", "ф")	set input to replace_chars(input, "x", "х")	set input to replace_chars(input, "c", "ц")	set input to replace_chars(input, "č", "ч")
	set input to replace_chars(input, "dž", "џ")	set input to replace_chars(input, "š", "ш")		set input to replace_chars(input, "A", "А")	set input to replace_chars(input, "B", "Б")	set input to replace_chars(input, "V", "В")	set input to replace_chars(input, "G", "Г")	set input to replace_chars(input, "D", "Д")
	set input to replace_chars(input, "Đ", "Ђ")	set input to replace_chars(input, "E", "Е")	set input to replace_chars(input, "Ž", "Ж")	set input to replace_chars(input, "Z", "З")	set input to replace_chars(input, "I", "И")	set input to replace_chars(input, "J", "Ј")	set input to replace_chars(input, "K", "К")	set input to replace_chars(input, "L", "Л")
