					<string>on run {input, parameters}		set input to input as string		set input to replace_chars(input, "a", "а")	set input to replace_chars(input, "b", "б")	set input to replace_chars(input, "v", "в")	set input to replace_chars(input, "g", "г")	set input to replace_chars(input, "d", "д")
